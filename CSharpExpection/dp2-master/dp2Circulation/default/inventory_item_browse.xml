<?xml version="1.0" encoding="utf-8"?>
<root>
  <col title="状态" type="item_state">
    <xpath>*/state</xpath>
  </col>
  <col title="册条码号" type="item_barcode">
    <xpath>*/barcode</xpath>
  </col>
  <col title="馆藏地点">
    <xpath>*/location</xpath>
  </col>
  <col title="索取号" type="call_number">
    <xpath>*/accessNo</xpath>
  </col>
  <col title="价格">
    <xpath>*/price</xpath>
  </col>
  <col title="注释">
    <xpath>*/comment</xpath>
  </col>
  <col title="借阅者" type="borrower">
    <xpath>*/borrower</xpath>
  </col>
  <col title="借阅日期" convert="rfc1123tolocaltimes" type="borrowDate">
    <xpath>*/borrowDate</xpath>
  </col>
  <col title="借阅期限">
    <xpath>*/borrowPeriod</xpath>
  </col>
  <col title="参考ID" type="item_refid">
    <xpath>*/refID</xpath>
  </col>
  <col title="流通次数">
    <xpath>*/borrowHistory/@count</xpath>
  </col>
  <col title="注释">
    <xpath>*/comment</xpath>
  </col>
  <col title="书目记录" type="parent_id">
    <xpath>*/parent</xpath>
  </col>
</root>