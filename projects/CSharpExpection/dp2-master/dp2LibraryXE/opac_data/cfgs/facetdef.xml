<?xml version="1.0" encoding="utf-8"?>
<facetDef>
  <facet id="初始结果集">
    <caption lang="zh-CN">初始结果集</caption>
    <caption lang="en-US">Origin Resultset</caption>
  </facet>
  <facet id="lang" aggregation="yes">
    <caption lang="zh-CN">语言</caption>
    <caption lang="en-US">Language</caption>
  </facet>
  <facet id="country" aggregation="yes">
    <caption lang="zh-CN">出版国</caption>
    <caption lang="en-US">Country of Publication</caption>
  </facet>
  <facet id="title">
    <caption lang="zh-CN">题名</caption>
    <caption lang="en-US">Title</caption>
  </facet>
  <facet id="author" aggregation="yes">
    <caption lang="zh-CN">责任者</caption>
    <caption lang="en-US">Author</caption>
  </facet>
  <facet id="publisher" aggregation="yes">
    <caption lang="zh-CN">出版者</caption>
    <caption lang="en-US">Publisher</caption>
  </facet>
  <facet id="publishdate" aggregation="publishyear">
    <caption lang="zh-CN">出版日期</caption>
    <caption lang="en-US">Publish Date</caption>
  </facet>
  <facet id="series" aggregation="yes">
    <caption lang="zh-CN">丛书</caption>
    <caption lang="en-US">Series</caption>
  </facet>
  <facet id="class-lcc" aggregation="class1">
    <caption lang="zh-CN">分类 - 美国国会图书馆分类法</caption>
    <caption lang="en-US">Class - LCC</caption>
  </facet>
  <facet id="class-udc" aggregation="class1">
    <caption lang="zh-CN">分类 - 国际十进分类法</caption>
    <caption lang="en-US">Class - UDC</caption>
  </facet>
  <facet id="class-ddc" aggregation="class1">
    <caption lang="zh-CN">分类 - 杜威十进分类法</caption>
    <caption lang="en-US">Class - DDC</caption>
  </facet>
  <facet id="class-clc" aggregation="class1">
    <caption lang="zh-CN">分类 - 中图法</caption>
    <caption lang="en-US">Class - CLC</caption>
  </facet>
  <facet id="class-ktf" aggregation="class1">
    <caption lang="zh-CN">分类 - 科图法</caption>
    <caption lang="en-US">Class - KTF</caption>
  </facet>
  <facet id="class-rdf" aggregation="class1">
    <caption lang="zh-CN">分类 - 人大法</caption>
    <caption lang="en-US">Class - RDF</caption>
  </facet>
  <facet id="subject" aggregation="yes">
    <caption lang="zh-CN">主题</caption>
    <caption lang="en-US">Subject</caption>
  </facet>
  <facet id="isbn">
    <caption lang="zh-CN">ISBN</caption>
    <caption lang="en-US">ISBN</caption>
  </facet>
  <facet id="issn">
    <caption lang="zh-CN">ISSN</caption>
    <caption lang="en-US">ISSN</caption>
  </facet>
  <facet id="size" aggregation="yes">
    <caption lang="zh-CN">尺寸</caption>
    <caption lang="en-US">Size</caption>
  </facet>

</facetDef>