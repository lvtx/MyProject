TABLE.total
{
/*	width: 970px;*/
	width: auto;

	margin-top: 10px;
	margin-bottom: 10px;
	margin-left: 10px;
	margin-right: 10px;
}

TABLE.total TD.left
{
	width: 15%;
	vertical-align: top;
}

TABLE.total TD.middle
{
	width: 16px;
}

TABLE.total TD.right
{
	width: 85%;
}


DIV.personalinfo_wrapper
{
	width: auto;
	border-width: none;
}

DIV.command_wrapper
{
	width: auto;
	border-width: none;
}

TABLE.command
{
	font-size: 12px;
	background-color: white;
	border-style: solid;
	border-top-width: 18px;
	border-bottom-width: 18px;
	border-left-width: 8px;
	border-right-width: 8px;
	border-color: whitesmoke;
	width: 100%;
}


TABLE.personalinfo
{
	font-size: 12px;
	text-align: left;
	background-color: white;
/*
	background-image: url(./title.jpg);
	background-attachment: fixed;
*/
	border-style: solid;
	border-top-width: 18px;
	border-bottom-width: 18px;
	border-left-width: 8px;
	border-right-width: 8px;
	border-color: whitesmoke;
	width: 100%;
}

TABLE.personalinfo TR.columntitle
{
	font-weight: bolder;
}

TABLE.personalinfo TR.dark
{
	background-color: #f0f0f0
}

TABLE.personalinfo TR.light
{

}

TABLE.personalinfo TD.summary
{
	font-size: 10px;
}

TABLE.personalinfo TD.name
{
	width: 20%;
	font-weight: bolder;
}

TABLE.personalinfo TD.name_value
{
	width: 80%;
	font-weight: bolder;
	font-size: 12px;
}

TABLE.personalinfo TD.barcode
{
	font-weight: bolder;
}

TABLE.personalinfo TD.barcode_value
{
	font-weight: bolder;
	font-size: 12px;
}



